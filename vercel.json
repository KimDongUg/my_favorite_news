{
  "redirects": [
    { "source": "/privacy", "destination": "/privacy.html", "permanent": true },
    { "source": "/terms", "destination": "/terms.html", "permanent": true },
    { "source": "/copyright", "destination": "/copyright.html", "permanent": true },
    { "source": "/feedback", "destination": "/feedback.html", "permanent": true }
  ],
  "rewrites": [
    {
      "source": "/((?!privacy\\.html|terms\\.html|copyright\\.html|feedback\\.html).*)",
      "destination": "/index.html"
    }
  ]
}
